{
    "name": "venzra-website",
    "version": "1.0.0-alpha.1",
    "author": "SYSEN LIMITED",
    "license": "UNLICENSED",
    "private": true,
    "scripts": {
        "start": "node dist/server/main.js",
        "lint": "tslint --force --project client/tsconfig.json && tslint --force --project server/tsconfig.json",
        "develop": "npm run build:server && concurrently -k -s first \"npm run watch:server\" \"npm run watch:client\"",
        "build": "npm run lint && npm run build:client && npm run build:server",
        "build:client": "ng build --prod --env=prod --output-path=dist/client",
        "build:server": "tsc -p ./server",
        "test": "npm run build && npm run test:server && concurrently -k -s first \"npm start\" \"npm run test:client\"",
        "test:client": "protractor ./client/test/protractor.conf.js",
        "test:server": "concurrently -k -s first \"mongod\" \"./node_modules/.bin/istanbul cover --report=none ./node_modules/.bin/cucumber-js -- ./server/test/features --require ./server/test && ./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/html-report -t html\"",
        "watch:client": "ng build -w --output-path=dist/client",
        "watch:server": "concurrently -k -s first \"tsc -w -p ./server\" \"nodemon dist/server/main.js\"",
        "postinstall": "./node_modules/.bin/webdriver-manager update --versions.chrome 2.31 --versions.standalone 3.4.0"
    },
    "dependencies": {
        "@angular/animations": "4.4.5",
        "@angular/cdk": "2.0.0-beta.12",
        "@angular/common": "4.4.5",
        "@angular/compiler": "4.4.5",
        "@angular/core": "4.4.5",
        "@angular/flex-layout": "2.0.0-beta.9",
        "@angular/forms": "4.4.5",
        "@angular/http": "4.4.5",
        "@angular/material": "2.0.0-beta.12",
        "@angular/platform-browser": "4.4.5",
        "@angular/platform-browser-dynamic": "4.4.5",
        "@angular/router": "4.4.5",
        "aws-sdk": "2.133.0",
        "body-parser": "1.18.2",
        "core-js": "2.5.1",
        "express": "4.16.2",
        "hammerjs": "2.0.8",
        "moment": "2.19.1",
        "mongoose": "4.12.1",
        "morgan": "1.9.0",
        "rxjs": "5.4.3",
        "uuid": "3.1.0",
        "winston": "2.4.0",
        "zone.js": "0.8.18"
    },
    "devDependencies": {
        "@angular/cli": "1.4.7",
        "@angular/compiler-cli": "4.4.5",
        "@angular/language-service": "4.4.5",
        "@types/aws-sdk": "2.7.0",
        "@types/express": "4.0.37",
        "@types/moment": "2.13.0",
        "@types/mongoose": "4.7.23",
        "@types/node": "8.0.34",
        "@types/uuid": "3.4.2",
        "chai": "4.1.2",
        "codelyzer": "3.2.1",
        "concurrently": "3.5.0",
        "cucumber": "2.3.1",
        "istanbul": "0.4.5",
        "nock": "9.0.22",
        "nodemon": "1.12.1",
        "protractor": "5.1.2",
        "protractor-cucumber-framework": "3.1.2",
        "remap-istanbul": "0.9.5",
        "shelljs": "0.7.8",
        "supertest": "3.0.0",
        "ts-node": "3.3.0",
        "tslint": "5.7.0",
        "typescript": "2.5.3"
    }
}
